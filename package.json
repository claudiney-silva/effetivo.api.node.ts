{
  "name": "app-site-api-node-ts",
  "version": "1.0.0",
  "description": "Website project on Node, TypeScript, Mongoose, ESLint, Prettier and Jest.",
  "main": "index.js",
  "scripts": {
    "start:dev": "ts-node-dev --respawn --inspect --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "start:prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/src/index.js",
    "start:local": "dotenv -e .env -- ts-node-dev --respawn --inspect --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "build": "tsc -p tsconfig.prod.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "node",
    "typescript",
    "mongoose",
    "eslint",
    "prettier",
    "jest",
    "passport"
  ],
  "author": "Claudiney Calixto da Silva",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/config": "0.0.38",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-rate-limit": "^5.1.1",
    "@types/http-status-codes": "^1.2.0",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.10.5",
    "@types/node": "^15.6.0",
    "@types/passport": "^1.0.6",
    "@types/passport-google-oauth20": "^2.0.7",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/pino": "^6.3.8",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/jest": "^26.0.23",
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "express-rate-limit": "^5.2.6",
    "http-status-codes": "^2.1.4",
    "jest": "^26.6.3",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.12.11",
    "node-cache": "^5.1.2",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pino": "^6.11.3",
    "swagger-autogen": "^2.9.1",
    "swagger-jsdoc": "^6.0.1",
    "swagger-ui-express": "^4.1.6",
    "ts-jest": "^26.5.5"
  },
